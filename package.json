    "docker:down": "yarn workspace frontend docker:down && yarn workspace backend docker:down",
    "docker:rebuild": "yarn workspace frontend docker:rebuild && yarn workspace backend docker:rebuild",
    "docker:clean": "yarn workspace backend docker:clean",
    "db:start": "yarn workspace backend db:start",
    "db:stop": "yarn workspace backend db:stop",
    "lint": "yarn workspace frontend lint"
