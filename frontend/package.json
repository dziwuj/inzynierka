{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --mode development",
    "build": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "docker:dev": "docker compose -f docker/docker-compose.yml up --build dev --remove-orphans",
    "docker:prod": "docker compose -f docker/docker-compose.yml up --build prod --remove-orphans",
    "docker:down": "docker compose -f docker/docker-compose.yml down --remove-orphans",
    "docker:rebuild": "docker compose -f docker/docker-compose.yml build --no-cache"
  },
  "dependencies": {
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.14",
    "axios": "^1.8.1",
    "mobx": "^6.13.6",
    "mobx-react": "^9.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.9.4",
    "react-toastify": "^11.0.5",
    "three": "^0.173.0",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.6.0",
    "workbox-broadcast-update": "^6.6.0",
    "workbox-cacheable-response": "^6.6.0",
    "workbox-core": "^6.6.0",
    "workbox-expiration": "^6.6.0",
    "workbox-google-analytics": "^6.6.1",
    "workbox-navigation-preload": "^6.6.0",
    "workbox-precaching": "^6.6.0",
    "workbox-range-requests": "^6.6.0",
    "workbox-routing": "^6.6.0",
    "workbox-strategies": "^6.6.0",
    "workbox-streams": "^6.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "@types/three": "^0.173.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "sass-embedded": "^1.93.2",
    "vite": "^6.0.11",
    "vite-plugin-mkcert": "^1.17.7",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-svgr": "^4.5.0"
  },
  "resolutions": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  }
}
